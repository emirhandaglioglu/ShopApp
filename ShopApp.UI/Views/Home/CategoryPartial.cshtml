@model IEnumerable<ShopApp.Entity.Concrete.Category>
<div class="col-md-3 col-md-push-9">
    <div class="sidebar-navigation">
        <div class="title">Kategoriler<i class="fa fa-angle-down"></i></div>
        <div class="list">
            @foreach (var item in Model)
            {
                <a class="entry" href="~/Category/GetCategory/@item.Id"><span><i class="fa fa-angle-right"></i>@item.CategoryName</span></a>
            }
        </div>
    </div>
    <div class="clear"></div>
</div>


<!-- end new collection directory -->